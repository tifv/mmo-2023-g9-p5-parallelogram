{
  "compilerOptions": {
    "module": "None",
    "target": "ES2020",
    "strict": true,
    "alwaysStrict": true,
    "outDir": "build",
    "outFile": "build/parallelogram.js",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "removeComments": true,
    "declaration": false,
    "sourceRoot": "/src/",
    "sourceMap": true,
  },
  "files": [
    "common.ts",
    "algebra/expr.ts",
    "algebra/sparse.ts",
    "algebra/solver.ts",
    "chooser.ts",
    "graph.ts",
    "flow.ts",
    "incut.ts",
    "main.ts",
  ]
}
